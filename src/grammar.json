{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "rustfmt",
  "rules": {
    "format_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "ALIAS",
                    "content": {
                      "type": "STRING",
                      "value": "{{"
                    },
                    "named": true,
                    "value": "escaped"
                  },
                  {
                    "type": "ALIAS",
                    "content": {
                      "type": "STRING",
                      "value": "}}"
                    },
                    "named": true,
                    "value": "escaped"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "format"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "text"
              }
            ]
          }
        }
      ]
    },
    "format": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "argument"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "STRING",
                    "value": ":"
                  },
                  "named": true,
                  "value": "colon"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SEQ",
                          "members": [
                            {
                              "type": "CHOICE",
                              "members": [
                                {
                                  "type": "SYMBOL",
                                  "name": "fill"
                                },
                                {
                                  "type": "BLANK"
                                }
                              ]
                            },
                            {
                              "type": "SYMBOL",
                              "name": "align"
                            }
                          ]
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "sign"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "#"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "STRING",
                            "value": "0"
                          },
                          "named": true,
                          "value": "number"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "SYMBOL",
                            "name": "count"
                          },
                          "named": true,
                          "value": "width"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "SEQ",
                            "members": [
                              {
                                "type": "STRING",
                                "value": "."
                              },
                              {
                                "type": "CHOICE",
                                "members": [
                                  {
                                    "type": "SYMBOL",
                                    "name": "count"
                                  },
                                  {
                                    "type": "STRING",
                                    "value": "*"
                                  }
                                ]
                              }
                            ]
                          },
                          "named": true,
                          "value": "number"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "type"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "escaped": {
      "type": "STRING",
      "value": "escaped"
    },
    "width": {
      "type": "STRING",
      "value": "width"
    },
    "number": {
      "type": "STRING",
      "value": "number"
    },
    "colon": {
      "type": "STRING",
      "value": "colon"
    },
    "fill": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^0]"
        },
        {
          "type": "STRING",
          "value": "0"
        }
      ]
    },
    "align": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": "^"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "sign": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "STRING",
          "value": "-"
        }
      ]
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "?"
        },
        {
          "type": "STRING",
          "value": "x?"
        },
        {
          "type": "STRING",
          "value": "X?"
        },
        {
          "type": "STRING",
          "value": "o"
        },
        {
          "type": "STRING",
          "value": "x"
        },
        {
          "type": "STRING",
          "value": "X"
        },
        {
          "type": "STRING",
          "value": "p"
        },
        {
          "type": "STRING",
          "value": "b"
        },
        {
          "type": "STRING",
          "value": "e"
        },
        {
          "type": "STRING",
          "value": "E"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "count": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "integer"
          },
          "named": true,
          "value": "number"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "parameter": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "argument"
        },
        {
          "type": "STRING",
          "value": "$"
        }
      ]
    },
    "argument": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\{\\}]*"
    },
    "integer": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[_a-zA-Z][_a-zA-Z0-9]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
